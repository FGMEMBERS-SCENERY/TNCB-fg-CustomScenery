<?xml version="1.0"?> <PropertyList> <path>TNCB_light_pole.ac</path> <animation> 
 <type>material</type> 
 <object-name>TNCB_light_pole</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>2</red>	
  <green>2</green>	
  <blue>2</blue>
 </emission>
</animation> 
<model>
	  <path>Models/Effects/RedLight10000.xml</path> 
	  <offsets> 
	     <x-m>0.14</x-m>
	     <y-m>0</y-m>
	     <z-m>21</z-m>
	     <heading-deg>0</heading-deg>
	  </offsets>
    </model>

    <model>
	  <path>Models/Effects/RedLight10000.xml</path> 
	  <offsets> 
	     <x-m>-0.14</x-m>
	     <y-m>0</y-m>
	     <z-m>21</z-m>
	     <heading-deg>0</heading-deg>
	  </offsets>
    </model>
</PropertyList>